{
  "name": "@sudtanj/nestjs-typegoose",
  "version": "10.1.4",
  "description": "A nestjs module wrapper for typegoose updated by sudtanj",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sudtanj/nestjs-typegoose.git",
    "directory": "deps/nestjs-typegoose"
  },
  "license": "MIT",
  "author": "Kyle Pfromer",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "commit": "git cz",
    "coverage": "yarn run test:cov && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint --fix ./",
    "prepare": "npm run build",
    "test": "jest --detectOpenHandles",
    "test:ci": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "resetMocks": true,
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "is-class": "0.0.9"
  },
  "devDependencies": {
    "@m8a/eslint-config": "^0.0.4",
    "@nestjs/common": "9.3.9",
    "@nestjs/core": "9.3.9",
    "@nestjs/platform-express": "~9.3.9",
    "@nestjs/testing": "9.3.9",
    "@rushstack/eslint-config": "3.2.0",
    "@typegoose/typegoose": "^11.0.0-beta.1",
    "@types/jest": "29.4.0",
    "@types/supertest": "2.0.12",
    "coveralls": "3.1.1",
    "eslint": "8.35.0",
    "husky": "8.0.3",
    "jest": "29.4.1",
    "mongodb-memory-server": "8.11.5",
    "mongoose": "^7.0.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.0",
    "superagent": "8.0.9",
    "supertest": "6.3.3",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.2.0",
    "@nestjs/core": "^9.2.0",
    "@typegoose/typegoose": "^10.3.2",
    "mongoose": "^6.0.0"
  },
  "engines": {
    "node": ">=16"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "har-validator": "5",
      "uuid": "3",
      "superagent": "*"
    }
  }
}
